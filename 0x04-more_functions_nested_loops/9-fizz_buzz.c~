#include "holberton.h"
/**
 * fizz_buzz - dont print 3 or 5 multiple
 *
 * Return: 1
 */
void fizz_buzz(void)
{
int i, j;
{
for(i = 1; i <= 100; i++)
{
if( (i % 3) == 0 && (i % 5) == 0)
{
printf(" FizzBuzz ");
}
else if ((i % 3) == 0)
{
printf(" Fizz");
}
else if ((i % 5) == 0)
{
printf(" Buzz ");
}
else
{
printf(" %d ", i);
}
}
printf('\n');
}
}
