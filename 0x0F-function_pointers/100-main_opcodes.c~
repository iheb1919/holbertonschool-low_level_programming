#include <stdio.h>
#include <stdlib.h>
/**
 * main - main function
 * @argc: inpot function
 * @argv: input function
 * Return: success
 */
int main(int argc, char **argv)
{
int i, b;
char *byte;
if (argc != 2)
{
printf("Error\n");
exit(1);
}
bytes = atoi(argv[1]);
if (bytes < 0)
{
printf("Error\n");
exit(2);
}
byte = (char *)main;
for (i = 0; i < bytes; i++)
{
printf("%02hhx ", byte[i]);
if (i < b - 1)
printf(" ");
}
printf("\n");
return (0);
}
