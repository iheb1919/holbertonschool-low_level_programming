#include <stdio.h>
#include "dog.h"
/**
 * print_dog - print atributes of dog struct
 *@d:dog struct
 */
void print_dog(struct dog *d)
{
if (d != NULL)
{
if(d->name == NULL)
d->name = "(nil)";
if(d->nowner == NULL)
d->owner = "(nil)";
}
 printf("Name: %s\nAge: %f\nOwner: %s\n",d->name,d->age,d->owner);
}
